{
  "name": "@openmrs/esm-api",
  "version": "1.0.0",
  "description": "The javascript module for interacting with the OpenMRS API",
  "main": "dist/openmrs-esm-api.js",
  "scripts": {
    "test": "jest --config jest.config.js",
    "start": "webpack-dev-server",
    "build": "webpack --mode=production",
    "typescript": "tsc",
    "prettier": "prettier src/**/* --write",
    "lint": "eslint src --ext ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm test && npm run typescript"
    }
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-api.git"
  },
  "keywords": [
    "openmrs"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-api/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-api#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-ts-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@types/jest": "^24.0.17",
    "fhir.js": "0.0.20",
    "lodash-es": "^4.17.15"
  }
}
